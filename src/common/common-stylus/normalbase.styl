.overlay-main
	background-color rgba(40, 40, 40, 0.8)
	width 100%
	height 100%
	z-index 101
	position fixed
	left 0
	top 0
	display none
	

.popup-wrap
	box-sizing border-box
    overflow-y: auto
	max-width 620px
	width 100%
	position fixed
	z-index 102
	top 50%
	left 50%
	transform: translate(-50%, -50%)
	background-color whiteColor
	padding 70px 70px 50px 70px
	display none

@media screen and (max-width 1280px)
    .popup-wrap
        position: fixed

@media screen and (max-width 767px)
    .popup-wrap
        // top 10px
        // left 10px
        // transform translate(0%, 0%)
        padding: 35px 20px
        max-width inherit
        width calc(100% - 20px)
        box-sizing border-box
    
        .popup-submit,
        .popup-submit.submit-tel
            height 40px
            font-size 12px

.popup-close
    position absolute
    right 10px
    top 10px
    width 30px
    height 30px
    display block
    background url('../img/main/close.jpg') center center no-repeat
    background-size cover
    background-repeat: no-repeat
    background-position: center
    z-index 103

    @media screen and (max-width 767px)
        width 20px
        height 20px

    &:hover
        cursor pointer

