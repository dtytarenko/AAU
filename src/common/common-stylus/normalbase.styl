:active, :hover, :focus {
    outline: 0;
    outline-offset: 0;
}

input {
    border-radius: 0
    -webkit-appearance none
    -webkit-border-radius 0px
}

input[type=radio]
    -webkit-appearance: none
    -moz-appearance: none
    appearance: none

body, main, html
    overflow-x: hidden

button
    a
        width 100%
        height 100%

a
    outline: none

h1
    font-size 36px
    font-family 'Helvetica-Bold', sans-serif
    text-transform: uppercase
    color semiViolet

    @media screen and (max-width 1280px)
        font-size 30px
        letter-spacing 1.5px
        text-align: center
    
    @media screen and (max-width 767px)
        font-size 22px
        letter-spacing: normal

.btn__generalstyle
    display flex
    justify-content center
    box-sizing border-box
    width 100%

.btn__medium    
    max-width 330px
    height 80px
    background-color semiPurple 
    color whiteColor
    border 2px solid semiPurple
    line-height 76px

    &:hover
        color semiPurple
        border 2px solid semiPurple
        background-color whiteColor

.heading
    width 100%

    @media screen and (max-width: 1280px) {
        align-self flex-start
    }

.separator
    margin 20px 0 40px 0
    position relative
    height 11px
    display flex
  
    &:after
        content ''
        display block
        height 1px
        width calc(100% - 20px)
        background-color themeColor
        position absolute
        top 5px
        right 0

        @media screen and (max-width: 767px) {
            width 100%
        }

    @media screen and (max-width: 767px) {
        justify-content center
        margin-top 10px
        margin-bottom 30px
    }


//////article
.stndrt__text
    font-family 'HelveticaNeue-Light'
    line-height 30px
    color semiBlack
    font-size 16px
    text-align justify
    margin-bottom 20px

    @media screen and (max-width: 767px) {
        text-align center
        line-height 22px
        font-size 16px
        margin-bottom 5px
    }

    &:not(:last-child)
        margin-bottom 40px

        @media screen and (max-width: 767px) {
            margin-bottom 20px
        }

//------------------------blog-------------------------

.blog-entry
    display flex
    margin-bottom 50px
    max-height: 230px
    justify-content: space-between

    @media screen and (max-width: 767px) {
        flex-direction column
        max-height: inherit
        justify-content center
    }

.blog-entry__wrapimage
    max-width 180px
    max-height 105px
    height 100%
    width 100%

    @media screen and (max-width: 767px){
        margin 0 auto 10px
    }

    img
        max-width 180px
        min-width 180px
        max-height inherit
        object-fit fill


.blog-entry__wraptext
    margin-left 40px
    display flex
    flex 1 1 auto
    flex-direction: column

    @media screen and (max-width: 1280px){
        width 100%
    }

.blog-entry__wraptext
        @media screen and (max-width: 510px) {
        margin 0
        }

.blog-entry__title
    font-weight 500
    font-family 'HelveticaNeue-Bold'
    font-size 18px
    color semiViolet
    margin-bottom 15px
    line-height 24px
    padding-right 15px
    overflow: hidden
    -o-text-overflow: ellipsis
    text-overflow: ellipsis
    display: -webkit-box
    -webkit-line-clamp: 2
    -webkit-box-orient: vertical


    @media screen and (max-width: 510px) {
        text-align center
        width 100%
        padding 0
        margin-top 20px
    }

.blog-entry__date
    font-family 'HelveticaNeue-Bold'
    font-size 14px
    color whiteColor
    line-height 24px
    letter-spacing 0.001em
    background url('../img/all/date-blog.png') no-repeat
    position relative
    margin-bottom 20px
    padding-left 22px

    @media screen and (max-width: 510px) {
        text-align center
        background-position center
        padding 0
    }

    &:after
        content ''
        display block
        height 1px
        width calc(100% - 110px)
        position absolute
        top 11px
        left 110px
        background-color semiPurple

        @media screen and (max-width: 510px) {
            width 100%
            left 0
            z-index -1
        }

.blog-entry__preview
    font-weight 300
    font-family 'HelveticaNeue-Light'
    font-size 14px
    color semiBlack
    line-height 20px
    letter-spacing 0.025em
    margin-bottom 15px
    letter-spacing 0.35px
    padding-right 25px
    max-height 60px
    text-overflow: ellipsis
    width: 100%
    overflow: hidden
    min-height: 60px
    max-height: 60px
    

    @media screen and (max-width: 510px) {
        text-align center
        padding 0
    }

.blog-entry__more
    font-family 'HelveticaNeue-Black'
    font-size 12px
    font-weight 400
    line-height 24px
    letter-spacing 0.01em
    display flex
    align-items center
    justify-content flex-end
    color semiPurple
    text-transform: uppercase
    
    @media screen and (max-width: 510px) {
        justify-content center
    }

    &:after 
        display block
        content ''
        right 50%
        top 0
        width 24px
        height 20px
        background url('../img/all/read-more.png')
        margin-left 10px

    &:hover
        opacity 0.7



.blog-entry__btn-group 
    display flex
    margin-top auto
    justify-content space-between
    align-items center
    
    .btn-m
        margin-left 20px

        @media screen and (max-width: 510px) {
            margin 10px 0 0 0
        }

    @media screen and (max-width: 510px) {
        flex-direction column
    }

    a
        @media screen and (max-width: 767px) {
            max-width none
            max-height: 60px
            height 60px
        }
    

.container
    max-width 1280px
    width 100%
    padding 60px 0 90px
    box-sizing border-box
    margin 0 auto
    display flex
    justify-content space-between

    @media screen and (max-width: 1280px) {
        flex-direction column
        align-items center
        padding 80px 35px
    }

    @media screen and (max-width: 767px) {
        padding 40px 20px
    }
 
