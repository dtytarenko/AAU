.typical-checkbox
    appearance none
    width: 23px
    height: 20px
    box-sizing: border-box
    z-index: 10
    margin 2px 10px 0 0

.typical-checkmark
    position: absolute
    top: 50%
    left 0%
    transform: translateY(-50%)
    content: ''
    display inline-block
    background: url(../img/all/dots.png) no-repeat
    width 23px
    height 20px

.typical-checkbox:checked + .typical-checkmark
    background url(../img/all/dots-active.png) no-repeat

.typical-checkbox:focus + .typical-checkmark
    background url(../img/all/dots-active.png) no-repeat


